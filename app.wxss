/**app.wxss**/
page {
  background-color: #F8F8F8;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
}

.container {
  display: flex;
  flex-flow: column nowrap;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

.page-body {
  width: 100%;
  flex-grow: 1;
  overflow-x: hidden;
}


.page-body-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

.page-head {
  display: flex;
  flex-flow: column nowrap;
  justify-content: center;
  align-items: center;
}

.page-head-line{
  margin: 0 auto;
  width: 150rpx;
  height: 2rpx;
  background-color: #D8D8D8;
}

.page-foot {
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
}

.icon-foot{
  width: 30rpx;
  height: 30rpx;
}

.weui-agree__link {
  display: inline;
  color: #586c94;
}
